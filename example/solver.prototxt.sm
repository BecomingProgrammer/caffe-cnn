snapshot: 10000
snapshot_prefix: "PLACEHOLDER_MODEL_STORE"
solver_mode: GPU
#solver_type: ADADELTA
#delta: OPTIMIZE{"type": "ENUM", "options": ["1e-8", "1e-7", "1e-6"]}
solver_type: ADAGRAD
#solver_type: NESTEROV
device_id: 2
