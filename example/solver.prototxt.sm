net: "PLACEHOLDER_NET"
test_iter: 25
test_interval: 200
base_lr: OPTIMIZE{"type": "ENUM", "options": ["0.1", "0.01","0.001"]}
#lr_policy: "step"
lr_policy: "fixed"
#gamma: 0.1
#stepsize: 2000
display: 200
max_iter: 5000
